# These environment variables are set by the build process and read by
# test/functional/test_runner.py and test/util/btcturquoise-util-test.py

[environment]
SRCDIR=@abs_top_srcdir@
BUILDDIR=@abs_top_builddir@
EXEEXT=@EXEEXT@
RPCAUTH=@abs_top_srcdir@/share/rpcauth/rpcauth.py

[components]
# Which components are enabled. These are commented out by `configure` if they were disabled when running config.
@ENABLE_WALLET_TRUE@ENABLE_WALLET=true
@BUILD_BTCTURQUOISE_UTILS_TRUE@ENABLE_UTILS=true
@BUILD_BTCTURQUOISED_TRUE@ENABLE_BTCTURQUOISED=true
@ENABLE_ZMQ_TRUE@ENABLE_ZMQ=true
